<!-- Google Analytics event tracking in Webflow -->
<!-- https://forum.webflow.com/t/implementing-google-analytics/58962/4 -->
<script>
    $(document).ready(function() {      
      $(document).on('click', '[data-gatrack]', function(e) {

        var $link = $(this);
                var commaSeperatedEventData = $link.data('gatrack');
                var eventParams = commaSeperatedEventData.split(',');

                if (!eventParams) { return; }

                eventCategory = eventParams[0]
                eventAction = eventParams[1]
                eventValue = eventParams[2]

                ga('send', {
                               hitType: 'event',
                               eventCategory: eventCategory,
                               eventAction: eventAction,
                               eventLabel: eventValue
                });
          
      });
    });
    </script><script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.4.1/js/swiper.min.js"></script>
<!--<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.9"></script>-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/1.1.1/typed.min.js"></script>


<script>
$(document).ready(function() {
	$('.home-sliders-mask-prevent-click').mousedown(function(e) {
		e.stopImmediatePropagation();
	});
  $('.home-sliders-mask-prevent-click').on('touchstart', (e) => {
  	e.stopImmediatePropagation();
  });    
	$('.home-testimonials-slider-native-nav').click(function() {
  	$('.home-testimonials-progress-bar').css('display', 'none');
  });
  
  const mySwiper = new Swiper ('.swiper-container', {
    effect: 'fade',
    fadeEffect: {
    	crossFade: false
  	},
    autoplay: true,
    speed: 450,
    touchRatio: 0,
    disableOnInteraction: false,
    noSwipingSelector: 'input',
    noSwiping: true,
    simulateTouch: false,
    shortSwipes: false,
    longSwipes: false,
    allowTouchMove: false,
    pagination: {
      el: '.swiper-pagination',
      clickable: true
    }
  });
  
  let [$leads, $projects, $partners, $investors, $support_tickets, 
  $candidates, $buyers_sellers] = 
  ['#leads', '#projects', '#partners', '#investors', '#support_tickets', 
  '#candidates', '#buyers_sellers'];
  
  const homeHeroGraphic = $('.home-hero-graphic');  
  
  let swiperBulletClicked = true; 
	const swiperBullet = $('.swiper-pagination-bullet'); 
	swiperBullet.on('click', function(){
  	swiperBulletClicked = false;
    const currentBullet = swiperBullet.index(this);
    const nthChildNum = currentBullet + 1;
    
    homeHeroGraphic.removeClass('home-hero-graphic-change');	// remove class from other home-hero graphics
    homeHeroGraphic.eq(currentBullet).addClass('home-hero-graphic-change');	// add to current home-hero graphic
    
    const element = $(this).closest('.swiper-pagination')
    .prev('.home-slider')
    .children(':nth-child('+nthChildNum+')')
    .find('.home-hero-text-change');
    const elemId = element.attr('id');    
    let typedString = elemId.replace(/_/,' ');
    if(elemId === 'buyers_sellers'){typedString = 'buyers & sellers'}
    
    runTyped('#'+elemId, [typedString]);
    $('#'+elemId).text('');	// clear text left in by typed
  });
  
  mySwiper.on('slideChange', function(e) {
  	if(swiperBulletClicked){	
  		homeHeroGraphic.removeClass('home-hero-graphic-change');	// remove class on every slide change    
  		if(mySwiper.activeIndex === 0){
  			runTyped($leads, ['leads','']); // empty string triggers backspace
      	homeHeroGraphic.eq(0).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 1){
  			runTyped($projects, ['projects','']);
      	homeHeroGraphic.eq(1).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 2){
  			runTyped($partners, ['partners','']);
      	homeHeroGraphic.eq(2).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 3){
  			runTyped($investors, ['investors','']);
      	homeHeroGraphic.eq(3).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 4){
  			runTyped($support_tickets, ['support tickets','']);
      	homeHeroGraphic.eq(4).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 5){
  			runTyped($candidates, ['candidates','']);
      	homeHeroGraphic.eq(5).addClass('home-hero-graphic-change');
    	}
    	else if(mySwiper.activeIndex === 6){
  			runTyped($buyers_sellers, ['buyers & sellers','']);
      	homeHeroGraphic.eq(6).addClass('home-hero-graphic-change');
    	}
    }
  });
  
  runTyped($leads, ['leads','']); // run typed on swiper init
 
 	function runTyped(elem, stringArr){
  	$(elem).typed({ 
  		strings: stringArr,
    	stringsElement: null,
  		typeSpeed: 60,
    	startDelay: 100,
      backDelay: 1200,
      backSpeed: 40,
      showCursor: false,	// using custom cursor defined in css
      cursorChar: '|',
    	loop: false,
    	attr: null,
    	contentType: 'text'
		});
    $(elem).removeData('typed'); // allow typed to run again
  }

});  
</script>

